{
  "manifest_version": 3,
  "name": "Remove Market Screener Free limits",
  "version": "0.1",
  "author": "PAU COLOME <paucolome10@gmail.com>",
  "description": "This extension removes Market Free limits",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "https://es.marketscreener.com/*",
        "https://www.marketscreener.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}